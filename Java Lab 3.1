import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int n = input.nextInt(); 
        
        for (int j = 0; j < n; j++) {
            int t = input.nextInt();

            if (t < 0) {
                System.out.println("Invalid input: negative index.");
                continue;
            }
            if (t == 0) {
                System.out.println(0);
                continue;
            }
            if (t == 1) {
                System.out.println(1);
                continue;
            }
            int[] ar = new int[t + 1];
            ar[0] = 0;
            ar[1] = 1;

            for (int i = 2; i <= t; i++) {
                ar[i] = ar[i - 1] + ar[i - 2];
            }

            System.out.println(ar[t]);
        }
    }
}
